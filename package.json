{
  "name": "potential-eureka",
  "version": "0.0.0-development",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "commit": "npx git-cz",
    "test": "mocha src/index-test.js -w",
    "test:single": "mocha src/index-test.js",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/talkor/potential-eureka.git"
  },
  "author": "Tal Koren (https://talkor.dev/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/talkor/potential-eureka/issues"
  },
  "homepage": "https://github.com/talkor/potential-eureka#readme",
  "dependencies": {
    "unique-random-array": "2.0.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "commitizen": "4.2.1",
    "cz-conventional-changelog": "3.0.2",
    "husky": "3.1.0",
    "mocha": "8.1.3",
    "semantic-release": "^17.1.1",
    "validate-commit-msg": "2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "build",
        "chore",
        "revert",
        "ci"
      ],
      "warnOnFail": false
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "npm run test:single"
    }
  }
}
